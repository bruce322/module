name: 屏蔽QUIC
desc: 防止哔哩哔哩等产生大量连接占用资源

script:
  shortcuts:
    quic: network == 'udp' and (dst_port == 443 or dst_port == 4483 or dst_port == 9305)
    bilibiliUdp: network == "udp" and ('bilibili' in host or 'biliapi' in host)
    youtubeUdp: network == 'udp' and ('googlevideo' in host or 'youtube' in host)
    
rules:
  - SCRIPT,quic,REJECT,no-track
  - SCRIPT,BilibiliUdp,REJECT,no-resovle
  - SCRIPT,youtubeUdp,REJECT,no-resovle
